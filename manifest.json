{
  "manifest_version": 2,

  "name": "Bloqueur de vidéo forcée DGFiP",
  "short_name": "DGFiPVideoBlocker",

  "version": "1.0.1",

  "icons": {
    "16": "images/icon-16.png",
    "48": "images/logo-48.png",
    "128": "images/logo-128.png"
  },

  "default_locale": "fr",
  "description": "__MSG_extension_description__",

  "content_scripts": [
    {
      "matches": ["https://*.impots.gouv.fr/*"],
	    "js": ["script.js"]
    }
  ],

  "author": "Nicolas Devenet",

  "permissions": [
    "activeTab"
  ]
}